{
  "name": "ronolo/photosort",
  "description": "Sorts images by date into a folder structure of YYYY/YYMM/YYMMDD structure",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Ronald Locke"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/src/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "require": {
    "psr/log": "1.0.2",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "symfony/console": "4.2.*",
    "symfony/dotenv": "4.2.*",
    "symfony/config": "~4.2",
    "symfony/contracts": "^1.0.2",
    "symfony/dependency-injection": "^4.2.5",
    "symfony/polyfill-mbstring": "~1.0",
    "symfony/filesystem": "~3.4|~4.0",
    "symfony/finder": "~3.4|~4.0",
    "symfony/yaml": "4.2.*",
    "symfony/process": "^4.2",
    "ext-json": "*",
    "jenssegers/imagehash": "^0.6.0",
    "symfony/framework-bundle": "^4.2"
  },
  "require-dev": {
    "phpunit/phpunit": "6.*"
  }
}
