{
  "name": "ronolo/photosort",
  "description": "Sorts images by date into a folder structure of YYYY/YYMM/YYMMDD structure",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Ronald Locke"
    }
  ],
  "autoload": {
    "classmap": [
      "src/",
      "bin/"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "require": {
    "psr/log": "1.0.2",
    "symfony/console": "^4.2",
    "symfony/process": "^4.2",
    "symfony/filesystem": "^4.2",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "6.*"
  }
}
